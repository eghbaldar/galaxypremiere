@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@using galaxypremiere.Application.Services.Users.Queries.GetUsers
@model ResultGetUsersServiceDto

@{
    ViewData["Title"] = "Galaxy Premiere - Users List";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<div class="card">
    <div class="card-header">
        <h4 class="card-title">Users List</h4>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table mb-0">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Name</th>
                        <th>Product</th>
                        <th>quantity</th>
                        <th>Status</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var user in Model.resultGetUsersServiceDto)
                    {
                        <tr>
                            <td>
                                <div class="round-img">
                                    <a href=""><img width="35" src="./images/avatar/1.png" alt=""></a>
                                </div>
                            </td>
                            <td>@user.Fullname</td>
                            <td>@user.Email</td>
                            <td><span>456 pcs</span></td>
                            <td><span class="badge badge-success">Done</span></td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>