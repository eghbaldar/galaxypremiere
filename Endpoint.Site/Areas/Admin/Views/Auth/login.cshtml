<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login - Galaxy Premiere</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    @*    <link rel="icon" type="image/png" href="images/icons/favicon.ico" />*@
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/admintemplate/logintemplate/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link href="~/admintemplate/logintemplate/fonts/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link href="~/admintemplate/logintemplate/fonts/iconic/css/material-design-iconic-font.min.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/admintemplate/logintemplate/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/admintemplate/logintemplate/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/admintemplate/logintemplate/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/admintemplate/logintemplate/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/admintemplate/logintemplate/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/admintemplate/logintemplate/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/admintemplate/logintemplate/css/main.css">
    <!-- Sweat Alert -->
    <link href="~/sweetalert/sweetalert2.min.css" rel="stylesheet" />
</head>
<body>
    <div class="limiter">
        <div class="container-login100" style="background-image: url('../../admintemplate/logintemplate/images/bg-01.jpg');">
            <div class="wrap-login100">
                <form class="login100-form validate-form">
                    <span class="login100-form-logo">
                        <i class="zmdi zmdi-landscape"></i>
                    </span>

                    <span class="login100-form-title p-b-34 p-t-27">
                        Log in
                    </span>

                    <div class="wrap-input100 validate-input" data-validate="Enter username">
                        <input class="input100" type="email"
                               id="txtEmail"
                               name="username" placeholder="Username">
                        <span class="focus-input100" data-placeholder="&#xf207;"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Enter password">
                        <input class="input100" type="password"
                               id="txtPass"
                               name="pass" placeholder="Password">
                        <span class="focus-input100" data-placeholder="&#xf191;"></span>
                    </div>

                    <div class="contact100-form-checkbox">
                        <input class="input-checkbox100" id="ckb1" type="checkbox" name="remember-me">
                        <label class="label-checkbox100" for="ckb1">
                            Remember me
                        </label>
                    </div>

                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn" type="button" onclick="Login()">
                            Login
                        </button>
                    </div>

                    <div class="text-center p-t-90">
                        <a class="txt1" href="#">
                            Forgot Password?
                        </a>
                    </div>
                </form>
            </div>
        </div>
    </div>


    <div id="dropDownSelect1"></div>
    <!-- JQuery-->
    <script src="~/lib/jquery/dist/jquery-3.7.1.min.js"></script>
        <!--===============================================================================================-->
    <script src="~/admintemplate/logintemplate/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/admintemplate/logintemplate/vendor/bootstrap/js/popper.js"></script>
    <script src="~/admintemplate/logintemplate/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/admintemplate/logintemplate/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/admintemplate/logintemplate/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/admintemplate/logintemplate/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/admintemplate/logintemplate/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="~/admintemplate/logintemplate/js/main.js"></script>
    <!-- WaitME -->
    <link href="~/waitme/waitme.css" rel="stylesheet" />
    <script src="~/waitme/waitmeking.js"></script>
    <script src="~/waitme/waitme.min.js"></script>
    <!-- Sweat Alert -->
    <script src="~/sweetalert/sweetalert2.all.min.js"></script>
    <script src="~/sweetalert/sweetalert2.min.js"></script>
    <script>
        function Login() {
            var postData = {
                'Email': $('#txtEmail').val(),
                'Password': $('#txtPass').val()
            };
            $.ajax({
                contentType: 'application/x-www-form-urlencoded',
                dataType: 'json',
                type: 'POST',
                data: postData,
                url: 'Login',
                success: function (data) {
                    if (data.isSuccess) {
                        location.replace('/admin/home/index');
                    }
                },
                error: function (request, status, error) {
                    alert('err:' + request.responseText);
                }
            });
        }
    </script>
</body>
</html>