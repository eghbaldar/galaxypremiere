<!DOCTYPE html>
<html lang="en">
<head>
    <!-- basic -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>@ViewData["PageTitle"]</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- site icon -->
    <link rel="icon" href="~/SiteTemplate/innerpages/innerpages/images/fevicon.png" type="image/png" />
    <!-- fontawesome icons -->
    <link href="~/sitetemplate/innerpages/fonts/fontawesome6/css/fontawesome.min.css" rel="stylesheet" />
    <link href="~/sitetemplate/innerpages/fonts/fontawesome6/css/brands.min.css" rel="stylesheet" />
    <link href="~/sitetemplate/innerpages/fonts/fontawesome6/css/solid.min.css" rel="stylesheet" />
    <!-- bootstrap css -->
    <link rel="stylesheet" href="~/SiteTemplate/innerpages/css/bootstrap.min.css" />
    @*<link rel="stylesheet" href="https://getbootstrap.com/docs/4.0/dist/css/bootstrap.min.css" />*@
    <!-- site css -->
    <link rel="stylesheet" href="~/SiteTemplate/innerpages/css/style.css" />
    <link rel="stylesheet" href="~/SiteTemplate/innerpages/css/animate.css" />
    <link rel="stylesheet" href="~/SiteTemplate/innerpages/css/flaticon.css" />
    <link rel="stylesheet" href="~/SiteTemplate/innerpages/css/font-awesome.min.css" />
    <!-- responsive css -->
    <link rel="stylesheet" href="~/SiteTemplate/innerpages/css/responsive.css" />
    <!-- color css -->
    <link rel="stylesheet" href="~/SiteTemplate/innerpages/css/colors.css" />
    <!-- select bootstrap -->
    <link rel="stylesheet" href="~/SiteTemplate/innerpages/css/bootstrap-select.css" />
    <!-- scrollbar css -->
    <link rel="stylesheet" href="~/SiteTemplate/innerpages/css/perfect-scrollbar.css" />
    <!-- custom css -->
    <link rel="stylesheet" href="~/SiteTemplate/innerpages/css/custom.css" />
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link href="~/lib/kingfuncs/general/general.css" rel="stylesheet" />
    @RenderSection("Styles", required:false)
</head>
<body class="dashboard dashboard_1">
    <div class="full_container">
        <div class="inner_container">
            <!-- Sidebar  -->
            @await Component.InvokeAsync("UserLeftSliderMenu")
            <!-- end sidebar -->
            <!-- right content -->
            <div id="content">
                <!-- topbar -->
                <div class="topbar">
                    <nav class="navbar navbar-expand-lg navbar-light">
                        <div class="full">
                            <button type="button" id="sidebarCollapse" class="sidebar_toggle"><i class="fa fa-bars"></i></button>
                            <div class="logo_section">
                                <a href="index.html"><img class="img-responsive" src="~/SiteTemplate/innerpages/images/logo/logo.png" alt="#" /></a>
                            </div>
                            <div class="right_topbar">
                                @await Component.InvokeAsync("TopRightUserPanel") <i class="mdi mdi-chevron-down"></i>
                            </div>
                        </div>
                    </nav>
                </div>
                <!-- end topbar -->
                <!-- dashboard inner -->
                @RenderBody()
                <!-- end dashboard inner -->
            </div>
        </div>
    </div>
    <!-- jQuery -->
    <script src="~/lib/jquery/dist/jquery-3.7.1.min.js"></script>
    <script src="~/SiteTemplate/innerpages/js/popper.min.js"></script>
    <script src="~/SiteTemplate/innerpages/js/bootstrap.min.js"></script>
    <!-- wow animation -->
    <script src="~/SiteTemplate/innerpages/js/animate.js"></script>
    <!-- select country -->
    <script src="~/SiteTemplate/innerpages/js/bootstrap-select.js"></script>
    <!-- owl carousel -->
    <script src="~/SiteTemplate/innerpages/js/owl.carousel.js"></script>
    <!-- chart js -->
    <script src="~/SiteTemplate/innerpages/js/Chart.min.js"></script>
    <script src="~/SiteTemplate/innerpages/js/Chart.bundle.min.js"></script>
    <script src="~/SiteTemplate/innerpages/js/utils.js"></script>
    <script src="~/SiteTemplate/innerpages/js/analyser.js"></script>
    <!-- nice scrollbar -->
    <script src="~/SiteTemplate/innerpages/js/perfect-scrollbar.min.js"></script>
    <script>
        var ps = new PerfectScrollbar('#sidebar');
    </script>
    <!-- custom js -->
    <script src="~/SiteTemplate/innerpages/js/custom.js"></script>
    <script src="~/SiteTemplate/innerpages/js/chart_custom_style1.js"></script>
    <!-- Sweat Alert -->
    <script src="~/sweetalert/sweetalert2.all.min.js"></script>
    <script src="~/sweetalert/sweetalert2.min.js"></script>
    <link href="~/sweetalert/sweetalert2.min.css" rel="stylesheet" />
    <!-- These validation libraries need to be included for unobtrusive validation -->
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <!-- WaitME -->
    <link href="~/waitme/waitme.css" rel="stylesheet" />
    <script src="~/waitme/waitmeking.js"></script>
    <script src="~/waitme/waitme.min.js"></script>
    <!-- King General -->
    <script src="~/lib/kingfuncs/general/general.js"></script>
    <!-- Render Sections -->
    @RenderSection("Scripts",required:false)
</body>
</html>
