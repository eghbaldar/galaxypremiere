                                                       placeholder="Grand Ave">
                                                </div>
                                                <div class="input-group mb-3">
                                                    <span class="input-group-text">City</span>
                                                    <input type="text"
                                                       class="form-control"
                                                       id="txtCompanyCity_@i"
                                                       value="@Model.ResultGetUserProfileCompaniesServiceDto.getUserProfileCompaniesDto[i].City"
                                                       placeholder="Miami">
                                                    <span class="input-group-text">State</span>
                                                    <input type="text"
                                                       class="form-control"
                                                       id="txtCompanyState_@i"
                                                       value="@Model.ResultGetUserProfileCompaniesServiceDto.getUserProfileCompaniesDto[i].State"
                                                       placeholder="Florida">
                                                </div>
                                                <div class="input-group mb-3">
                                                    <span class="input-group-text">PostCode</span>
                                                    <input type="text"
                                                       class="form-control"
                                                       id="txtCompanyPostCode_@i"
