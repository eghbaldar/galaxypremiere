                                                       placeholder="@@galaxypremiere">
                                                </div>
                                                <div class="input-group mb-3">
                                                    <span class="input-group-text">Twitter ID</span>
                                                    <input type="text"
                                                       class="form-control"
                                                       id="txtCompanyTwitter_@i"
