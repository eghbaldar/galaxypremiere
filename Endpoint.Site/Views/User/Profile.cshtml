<div id="TabUserAttachments" class="TabcontentClass">
    <div class="card">
        <div class="card-header">
            <h4 class="card-title">Attachments</h4>
        </div>
        <div class="card-body">
            <div class="basic-form">
                <form asp-action="Create" id="frmAttachments" method="post">
                    <div class="input-group mb-3">
                        <span class="input-group-text">Title</span>
                        <input type="text"
                               class="form-control"
                               id="txtAttachmentTitle"
                               placeholder="The Title of Attachment">
                    </div>
                    <div class="mb-3">
                        <input onchange="fileAttachmentChange(this);"
                               class="form-control form-control-lg" id="fileAttachment" type="file">
                    </div>

                    <div id="myBar" class="w3-container w3-green w3-center" style="width:0%">
                    </div>


                    <div class="mb-3" id="divInformationFileUpload">
                        Acceptable Formats: *.txt", *.docx, *.doc, *.pdf", *.jpg, *.png | Maximum Volume: 2MB
                    </div>
                    <div class="input-group mt-3">
                        <button type="button" onclick="PostProfileAttachments();"
                                id="btnUpdateProfileAttachments" class="btn cur-p btn-primary" disabled>
                            Add A New Attachment
                        </button>
                    </div>
                </form>
            </div>
        </div>
        <div class="card-body" id="divAttachments">
            @for (int i = 0; i <= Model.ResultGetUserProfileAttachmentsServiceDto.getUserProfileAttachmentsServiceDto.Count - 1; i++)
            {
                <fieldset>
                    <legend>Attachment</legend>
                    <table class="table table-striped align-middle">
                        <thead>
                            <tr>
                                <th>Title</th>
                                <th>Added Date</th>
                                <th>Downloads</th>
                                <th>File</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
