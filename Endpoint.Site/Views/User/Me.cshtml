<div id="TabUserBIO" class="TabcontentClass">
    <div class="card">
        <div class="card-header">
            <h4 class="card-title">ABOUT ME</h4>
        </div>
        <div class="card-body">
            <div class="basic-form">
                <form asp-action="UpdateBIO" id="frmBIO" method="post">

                    <div class="input-group mb-3">
                        <span class="input-group-text">Position (s) </span>
                        <div class="form-floating">
                            <div class="p-3">

                            <input type="text"
                                
                                id="kingTaggerPositions"
                                data-Iaction="../User/Positions"
                                data-elementValueTitle="position"
                                data-elementKeyTitle="id"
                                data-items=@if (Model.getUsersInformationServiceDto!= null)
                                    @Model.getUsersInformationServiceDto.Position 
                            >

                            </div>
                            <div class="ps-3 clickOnSuggestion">
                            <a  class="text-secondary" onclick="openDialougePostPosition();">I WOULD LIKE TO SUGGEST A NEW POSITION TO GALAXYPREMEIRE SYSTEM</a>
                            </DIV>
                            
                        </div>
                    </div>

                    <div class="input-group mb-3">
                        <span class="input-group-text">Introduction</span>
                        <div class="form-floating">
                            <textarea maxlength="300"
                                      style="height:200px"
                                      class="form-control"
                                      onkeyup="updateCounter(this,'300');"
                                      id="txtIntroduction">@{
                                    if (Model.getUsersInformationServiceDto != null)@Model.getUsersInformationServiceDto.Introduction
                                }</textarea>

                            <span class="form-label-counter" id="txtIntroductionCounter">0 / 300</span>
                        </div>
                    </div>
                    <div class="input-group mb-3">
                        <span class="input-group-text">BIO (Resume)</span>
                        <div class="form-floating">
                            <textarea maxlength="700"
                                      row="5"
                                      style="height:200px"
                                      class="form-control"
                                      onkeyup="updateCounter(this,'7000');"
                                      id="txtBIO">@{
                                    if (Model.getUsersInformationServiceDto != null)@Model.getUsersInformationServiceDto.BIO
                                }</textarea>

                            <span class="form-label-counter" id="txtBIOCounter">0 / 7000</span>
                        </div>
                    </div>
                    <div class="input-group mb-3">
                        <span class="input-group-text">Note</span>
                        <div class="form-floating">
                            <textarea maxlength="700"
                                      row="5"
                                      style="height:200px"
                                      class="form-control"
                                      onkeyup="updateCounter(this,'7000');"
                                      id="txtNote">@{
                                    if (Model.getUsersInformationServiceDto != null)@Model.getUsersInformationServiceDto.Note
                                }</textarea>

                            <span class="form-label-counter" id="txtNoteCounter">0 / 7000</span>
                        </div>
                    </div>
                    <button type="button" onclick="UpdateInfoBIO()" id="btnUpdateBIO" class="btn cur-p btn-primary">
                        UPDATE
                    </button>
                </form>
            </div>
        </div>
    </div>
</div>

