                    <input id="filedropzoneHeader" type="file" />
                </div>
                @*===================*@
            </div>
        </div>
    </div>
</div>

<div id="TabUserPrivacy" class="TabcontentClass">
    @*Username*@
    <div class="card">
        <div class="card-header">
            <h4 class="card-title">USERNAME</h4>
        </div>
        <div class="card-body">
            <div class="basic-form">
                <div class="row">
                    <div class="card-body">
                        <div class="basic-form">
                            <form>
                                <div class="form-row align-items-center">
                                    <div class="col-auto">

                                        <label class="sr-only">Username</label>
                                        @{
                                            if (Model.getUsersInformationServiceDto != null)
                                            {
                                                <span class="pb-3" id="spanCurrentUsername">
                                                    <a href="https://galaxypremiere.com/@Model.getUsersInformationServiceDto.Username" target="target">
                                                        www.galaxypremiere.com/@Model.getUsersInformationServiceDto.Username
                                                    </a>
                                                </span>
                                            }
                                            else
                                            {
                                                <span class="pb-3" id="spanCurrentUsername">
                                                    www.galaxypremiere.com/
                                                </span>
                                            }
                                        }

                                        <i id="iconCurrentUsername"></i>
                                        <div class="input-group mb-2">
                                            <span class="input-group-text">@@</span>
                                            <input type="text"
                                                   value="@{
                                                   if(Model.getUsersInformationServiceDto!=null)  
                                                        @Model.getUsersInformationServiceDto.Username
                                                   }"
                                                   data-firstValue="@{
                                                   if(Model.getUsersInformationServiceDto!=null)  
                                                        @Model.getUsersInformationServiceDto.Username
                                                   }"
                                                   id="txtUsername"
                                                   maxlength="50"
                                                   onkeyup="ChangeRuntimeUsername();"
                                                   class="form-control"
                                                   placeholder="Username">
                                        </div>
                                        @*bubble on the field*@
                                        <div class="bubble" id="bubble">
                                            <span class="pointer"></span>
                                            <span class="close" onclick="closeBubble()">&times;</span>
                                            <p id="bubbleText">To have a dedicated profile, it is necessary to assign a unique username for yourself at first.</p>
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <button type="button" onclick="UpdateInfoUsername();" id="btnUpdateUsername" class="btn btn-light mb-2">Update</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <br />
    @*Password*@
    <div class="card">
        <div class="card-header">
            <h4 class="card-title">PASSWORD</h4>
        </div>
        <div class="card-body">
            @*############################## KING PASSWORD #############################*@
            <div class="pb-3">
                <input type="checkbox" id="chkPassword" onchange="PasswordCheckbox(this)" />
                <label>Change Password</label>
                <span class="text-danger"></span>
            </div>
            <div class="input-group mb-3">
                <span id="spanPassNotMatch" class="input-group-text"> <i class="fa fa-lock"></i> </span>
                <input type="password"
                       onkeyup="checkPassMatchRePass('#txtPassword','#txtRePassword','#spanInvalidRePassword')"
                       id="txtPassword"
                       class="form-control"
                       disabled>
                <span class="input-group-text">
                    <div id="meter"></div>
                </span>
                <span id="spanGeneratePass"
                      class="input-group-text" onclick="generatePass('#spanInvalidRePassword','#txtPassword','#txtRePassword')"
                      style="pointer-events:none;">
                    <i class="fa fa-bolt"></i>
                </span>
                <span class="input-group-text">
                    <i id="spanPassVisibilty" onclick="showPassword('txtPassword','#spanPassVisibilty')" class="fa fa-eye-slash"></i>
                </span>
                <div id="val-password1-error" class="invalid-feedback animated fadeInUp" style="display: block;"></div>
            </div>
            <div id="spanInvalidRePassword" class="text-danger"></div>
            <div class="input-group mb-3">
                <span class="input-group-text">re-Pass</span>
                <input type="password"
                       id="txtRePassword"
                       onkeyup="checkPassMatchRePass('#txtPassword','#txtRePassword','#spanInvalidRePassword')"
                       class="form-control"
                       disabled>
            </div>
            <div style="padding-bottom:15px;">
                <a href="#">I can't remember my password! (reset password)</a>
            </div>
            <button type="button" onclick="UpdateInfoPassword('#txtPassword','#txtRePassword','#spanInvalidRePassword');" id="btnUpdatePassword" class="btn btn-primary mb-2">Update</button>
            @*############################## END of KING PASSWORD #############################*@
        </div>
    </div>
